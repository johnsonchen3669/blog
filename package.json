{
  "name": "astro-antfustyle-theme",
  "version": "2.3.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "dev": "astro dev",
    "check": "astro check",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier . --check --ignore-unknown",
    "format:write": "prettier . --write --ignore-unknown --cache",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --fix --cache",
    "toolbar:on": "astro preferences enable devToolbar",
    "toolbar:off": "astro preferences disable devToolbar",
    "prepare": "simple-git-hooks",
    "postbuild": "pagefind --site dist --exclude-selectors \"pre\" --include-characters \"<>\" && del-cli \"dist/pagefind/*ui*\"",
    "deploy": "bun run build && gh-pages -d dist --nojekyll"
  },
  "devDependencies": {
    "@ascorbic/feed-loader": "^2.0.1",
    "@astrojs/check": "^0.9.6",
    "@astrojs/markdown-remark": "^6.3.10",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "@atproto/api": "^0.19.0",
    "@eslint/js": "^10.0.1",
    "@expressive-code/plugin-collapsible-sections": "^0.41.7",
    "@expressive-code/plugin-line-numbers": "^0.41.7",
    "@iconify/json": "^2.2.443",
    "@types/node": "^25.3.2",
    "@types/nprogress": "^0.2.3",
    "@types/p5": "^1.7.7",
    "@typescript-eslint/parser": "^8.56.1",
    "@unocss/reset": "66.6.2",
    "astro": "^5.18.0",
    "astro-expressive-code": "^0.41.7",
    "astro-loader-bluesky-posts": "^1.2.3",
    "astro-loader-github-prs": "^1.3.1",
    "astro-loader-github-releases": "^2.1.1",
    "astro-robots-txt": "^1.0.0",
    "chalk": "^5.6.2",
    "del-cli": "^7.0.0",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^17.3.0",
    "html-entities": "^2.6.0",
    "lint-staged": "^16.2.7",
    "mdast-util-to-string": "^4.0.0",
    "nprogress": "^0.2.0",
    "p5": "^2.2.2",
    "pagefind": "^1.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-callouts": "^2.1.2",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-wrap-all": "^1.1.0",
    "remark-directive": "^4.0.0",
    "remark-directive-sugar": "^2.0.0",
    "remark-imgattr": "^1.1.1",
    "remark-math": "^6.0.0",
    "satori": "^0.19.2",
    "satori-html": "^0.3.2",
    "sharp": "^0.34.5",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "unist-util-visit": "^5.1.0",
    "unocss": "66.6.2",
    "viewerjs": "^1.11.7"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint -f json"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "simple-git-hooks"
    ]
  },
  "engines": {
    "node": ">=22.0.0 <23"
  },
  "dependencies": {
    "gh-pages": "^6.3.0"
  }
}
